		{
			"name": "Get Events",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "37ff3b57-239f-480e-9828-032406ddd234",
						"exec": [
							"pm.environment.set(\"id\", \"1\");",
							"pm.sendRequest({ url: \"http://localhost:7000/events\", method: \"POST\" });"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "8ba983e9-398a-43df-ae52-45d350082a4a",
						"exec": [
							"pm.test(\"Status code is 200\", () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Response JSON is a list\", () => {",
							"    pm.expect(pm.response.json()).to.be.an(\"array\");",
							"});",
							"pm.test(\"Items have identifier, title, start and end times, description, hosts, userID, and GoogleCal\", () => {",
							"    pm.response.json().forEach(event => {",
							"        pm.expect(event).to.have.all.keys(\"id\",\"title\", \"startTime\", \"endTime\", \"description\", \"hosts\", \"userID\", \"isGoogleEvent\");",
							"    });",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
