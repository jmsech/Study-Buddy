				{
					"listen": "test",
					"script": {
						"id": "e58af215-e065-4b0c-857c-66a3299395ad",
						"exec": [
							"pm.test(\"Status code is 201\", () => {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Identifiers are always increasing\", () => {",
							"    pm.sendRequest(\"http://localhost:7000/1/events\", (err, response) => {",
							"        const identifiers = response.json().map(event => event.id).reverse();",
							"        pm.expect(identifiers[0]).to.be.equal(identifiers[1] + 1);",
							"    });",
							"});",
							"",
							"pm.test(\"Data inserted is valid\", () => {",
							"    pm.sendRequest(\"http://localhost:7000/1/events\", (err, response) => {",
							"        pm.expect(response.json().last().title).to.equal(\"Postman title\");",
							"        pm.expect(response.json().last().description).to.equal(\"This is an event for our postman POST test\");",
							"    });",
							"});"
						],
						"type": "text/javascript"
					}
				}
